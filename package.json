{
  "name": "satimoto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "npm run protobuf",
    "protobuf": "pbjs -t static-module -w es6 --force-long -o proto/proto.js proto/rpc.proto proto/stateservice.proto proto/walletunlocker.proto proto/autopilotrpc/autopilot.proto proto/chainrpc/chainnotifier.proto proto/invoicesrpc/invoices.proto proto/routerrpc/router.proto proto/signrpc/signer.proto proto/walletrpc/walletkit.proto && pbts -o proto/proto.d.ts proto/proto.js",
    "react:clean": "rm -rf /tmp/metro-* && yarn start --reset-cache",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:regtest": "react-native run-android --variant networkregtestRelease --appIdSuffix regtest",
    "android:regtest-debug": "react-native run-android --variant networkregtestDebug --appIdSuffix regtest.debug",
    "android:testnet": "react-native run-android --variant networktestnetRelease --appIdSuffix testnet",
    "android:testnet-debug": "react-native run-android --variant networktestnetDebug --appIdSuffix testnet.debug",
    "android:mainnet": "react-native run-android --variant networkmainnetRelease",
    "android:mainnet-debug": "react-native run-android --variant networkmainnetDebug --appIdSuffix debug",
    "android:bundle-release": "cd android && ./gradlew clean && ./gradlew bundleNetworkmainnetRelease && ./gradlew bundleTestnetRelease",
    "android:bundle-release-mainnet": "cd android && ./gradlew clean && ./gradlew bundleNetworkmainnetRelease",
    "android:bundle-release-testnet": "cd android && ./gradlew clean && ./gradlew bundleNetworktestnetRelease",
    "ios": "react-native run-ios",
    "ios:podinstall": "cd ios && pod install",
    "ios:regtest": "react-native run-ios --configuration ReleaseRegtest",
    "ios:regtest-debug": "react-native run-ios --configuration DebugRegtest",
    "ios:testnet": "react-native run-ios --configuration ReleaseTestnet",
    "ios:testnet-debug": "react-native run-ios --configuration DebugTestnet",
    "ios:mainnet": "react-native run-ios --configuration Release",
    "ios:mainnet-debug": "react-native run-ios --configuration Debug"
  },
  "dependencies": {
    "@bitcoin-design/bitcoin-icons-react-native": "../Bitcoin-Icons/react-native",
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-mapbox-gl/maps": "^8.5.0",
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/elements": "^1.1.0",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "byte-base64": "^1.1.0",
    "fast-sha256": "^1.3.0",
    "mobx": "^6.3.3",
    "mobx-persist-store": "^1.0.3",
    "mobx-react": "^7.2.0",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-build-config": "^0.3.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.2.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-secure-storage": "^0.1.2",
    "react-native-securerandom": "^1.0.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "eslint": "^7.14.0",
    "events": "^3.3.0",
    "jest": "^26.6.3",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "protobufjs": "^6.11.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "readable-stream": "^3.6.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
